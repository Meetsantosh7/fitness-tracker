    <!-- Leaf 1 veins -->
    <path d="M130 145 Q145 150 165 160"/>
    <path d="M135 155 Q145 160 155 165"/>
    <path d="M130 165 Q140 168 150 170"/>
    
    <!-- Leaf 2 veins -->
    <path d="M170 135 Q185 140 205 150"/>
    <path d="M175 145 Q185 150 195 155"/>
    <path d="M170 155 Q180 158 190 160"/>
    
    <!-- Leaf 3 veins -->
    <path d="M200 155 Q215 160 235 170"/>
    <path d="M205 165 Q215 170 225 175"/>
    <path d="M200 175 Q210 178 220 180"/>
    
    <!-- Leaf 4 veins -->
    <path d="M150 175 Q165 180 185 190"/>
    <path d="M155 185 Q165 190 175 195"/>
    <path d="M150 195 Q160 198 170 200"/>
    
    <!-- Leaf 5 veins -->
    <path d="M220 175 Q235 180 255 190"/>
    <path d="M225 185 Q235 190 245 195"/>
    <path d="M220 195 Q230 198 240 200"/>
