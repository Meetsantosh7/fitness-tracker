  <!-- Highlights on leaves -->
  <g fill="#81C784" opacity="0.6">
    <ellipse cx="145" cy="150" rx="8" ry="5" transform="rotate(-15 145 150)"/>
    <ellipse cx="185" cy="140" rx="9" ry="6" transform="rotate(10 185 140)"/>
    <ellipse cx="215" cy="165" rx="8" ry="5" transform="rotate(-20 215 165)"/>
    <ellipse cx="165" cy="185" rx="7" ry="4" transform="rotate(25 165 185)"/>
    <ellipse cx="235" cy="185" rx="8" ry="5" transform="rotate(-10 235 185)"/>
  </g>
  
  <!-- Leaf stems -->
  <g stroke="#2E7D32" stroke-width="2" fill="none">
    <line x1="120" y1="165" x2="110" y2="175"/>
    <line x1="160" y1="155" x2="150" y2="165"/>
    <line x1="190" y1="175" x2="180" y2="185"/>
    <line x1="140" y1="195" x2="130" y2="205"/>
    <line x1="210" y1="195" x2="200" y2="205"/>
