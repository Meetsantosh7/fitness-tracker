    <circle cx="165" cy="155" r="0.8"/>
    <circle cx="195" cy="145" r="0.6"/>
    <circle cx="215" cy="175" r="0.9"/>
    <circle cx="155" cy="190" r="0.7"/>
    <circle cx="250" cy="175" r="0.8"/>
  </g>
  
  <!-- Some torn/natural leaf edges -->
  <g fill="url(#spinachGrad)" opacity="0.9">
    <path d="M175 175 Q180 172 185 175 Q180 178 175 175"/>
    <path d="M205 145 Q210 142 215 145 Q210 148 205 145"/>
    <path d="M245 165 Q250 162 255 165 Q250 168 245 165"/>
