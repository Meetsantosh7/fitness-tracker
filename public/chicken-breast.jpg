<!-- This is a placeholder - in a real implementation, this would be an actual JPG image file -->
<!-- For now, I'll create an enhanced SVG that looks very photo-realistic -->
<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Multiple gradients for realistic meat texture -->
    <radialGradient id="chickenGradient1" cx="0.3" cy="0.3" r="0.8">
      <stop offset="0%" style="stop-color:#ffeaa7;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#fdcb6e;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#e17055;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d63031;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="chickenGradient2" cx="0.7" cy="0.6" r="0.6">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#fdcb6e;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#e17055;stop-opacity:0.4" />
    </radialGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="3" dy="6" stdDeviation="4" flood-opacity="0.3"/>
    </filter>
    <filter id="texture" x="0%" y="0%" width="100%" height="100%">
      <feTurbulence baseFrequency="0.9" numOctaves="4" result="noise"/>
      <feBlend in="SourceGraphic" in2="noise" mode="multiply" result="blend"/>
      <feColorMatrix type="saturate" values="1.2" in="blend"/>
    </filter>
  </defs>
  
  <!-- Plate background -->
  <ellipse cx="200" cy="250" rx="180" ry="35" fill="#f8f9fa" stroke="#dee2e6" stroke-width="2" opacity="0.8"/>
  
  <!-- Main chicken breast shape -->
  <path d="M120 120 Q140 100 180 110 Q220 105 260 125 Q280 140 275 170 Q270 200 250 220 Q200 240 150 235 Q120 225 110 190 Q115 150 120 120 Z" 
        fill="url(#chickenGradient1)" 
        filter="url(#texture)"
        stroke="#d63031" 
        stroke-width="0.5"/>
  
  <!-- Highlight on top -->
  <path d="M140 130 Q160 120 200 125 Q240 130 250 150 Q245 170 220 180 Q180 185 150 175 Q135 160 140 130 Z" 
        fill="url(#chickenGradient2)" 
        opacity="0.7"/>
  
  <!-- Grill marks -->
  <line x1="140" y1="140" x2="240" y2="160" stroke="#8b4513" stroke-width="3" opacity="0.8"/>
  <line x1="145" y1="160" x2="245" y2="180" stroke="#8b4513" stroke-width="3" opacity="0.8"/>
  <line x1="150" y1="180" x2="230" y2="195" stroke="#8b4513" stroke-width="2" opacity="0.6"/>
  
  <!-- Small garnish - herbs -->
  <g transform="translate(280, 200)">
    <ellipse cx="0" cy="0" rx="8" ry="3" fill="#27ae60" opacity="0.9"/>
    <ellipse cx="5" cy="-2" rx="6" ry="2" fill="#2ecc71" opacity="0.8"/>
    <ellipse cx="-3" cy="1" rx="5" ry="2" fill="#27ae60" opacity="0.7"/>
  </g>
  
  <!-- Realistic juice/moisture -->
  <circle cx="190" cy="200" r="2" fill="#ffffff" opacity="0.6"/>
  <circle cx="210" cy="180" r="1.5" fill="#ffffff" opacity="0.5"/>
  <ellipse cx="170" cy="190" rx="3" ry="1" fill="#ffffff" opacity="0.4"/>
</svg>
